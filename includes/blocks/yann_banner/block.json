{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "yannyann-box/banner",
	"title": "Banner Box",
	"icon": "align-full-width",
	"category": "common",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "span"
		},
		"mediaID": {
			"type": "number"
		},
		"mediaURL": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		}
	},
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/style-index.css",
	"style": "file:./build/index.css"
}
